<%= link_to "Logo", root_path %>

<% if logged_in? %>
    <%= link_to "Profile", current_user %> <!-- or user_path(current_user) -->
    <%= link_to  "Settings", '#' %>
    <%= link_to  "Log Out", logout_path, method: :delete %>
<% else %>
    <%= link_to "Sign Up", signup_path %>
    <%= link_to "Log In", login_path %>
<% end %>
